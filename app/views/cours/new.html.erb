
<div class="container">
    <div class="shadow p-3 m-5 bg-white rounded text-center ">
      <h1>Nouveau cours</h1>

      <%= simple_form_for @cour do |f| %>
        <%= f.input :title %>
        <%= f.input :stars, collection: 0..5 %>
        <%= f.input :content%>
        <%= f.button :submit %>
      <% end %>
    </div>
</div>
  
